{ lib, pkgs, ... }:
{
  services.xserver = {
    enable = true;
    desktopManager = {
       #default = "plasma5";
       xterm.enable = false;
       plasma5.enable = true;
    };
    displayManager = {
        defaultSession = "plasma5";
    };
  };
  programs.ssh.askPassword = pkgs.lib.mkForce "${pkgs.plasma5.ksshaskpass.out}/bin/ksshaskpass";
}
